{
  "name": "roboshot",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "package-win": "electron-packager . Roboshot --overwrite --asar --platform=win32 --arch=ia32 --icon=./img/beer.ico --prune=true --out=Roboshot --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Roboshot\""
  },
  "devDependencies": {
    "asar": "^2.0.1",
    "electron": "^6.0.11",
    "electron-packager": "^14.0.6",
    "electron-rebuild": "^1.10.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "fs": "0.0.1-security",
    "jquery": "^3.4.1",
    "password-hash": "^1.2.2",
    "path": "^0.12.7",
    "popper.js": "^1.15.0",
    "python-shell": "^1.0.8",
    "serialport": "^8.0.5"
  }
}
